<?xml version="1.0" ?>
<sdf version="1.8">
  <world name="param_test">

    <!-- Physics -->
    <physics name="1ms" type="ignored">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
    </physics>

    <!-- Plugins -->
    <plugin filename="gz-sim-physics-system" name="gz::sim::systems::Physics"/>
    <plugin filename="gz-sim-user-commands-system" name="gz::sim::systems::UserCommands"/>
    <plugin filename="gz-sim-scene-broadcaster-system" name="gz::sim::systems::SceneBroadcaster"/>
    <plugin filename="gz-sim-sensors-system" name="gz::sim::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>
    <plugin filename="gz-sim-imu-system" name="gz::sim::systems::Imu"/>
    <plugin filename="gz-sim-contact-system" name="gz::sim::systems::Contact"/>

    <!-- Lighting -->
    <light type="directional" name="sun">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <direction>-0.5 0.1 -0.9</direction>
    </light>

    <!-- Ground Plane -->
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.5 0.5 0.5 1.0</ambient>
            <diffuse>0.5 0.5 0.5 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ==================== 외벽 (20x20m 방) ==================== -->
    <model name="wall_north">
      <static>true</static>
      <pose>0 10 0.5 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>20 0.1 1.0</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>20 0.1 1.0</size></box></geometry>
          <material>
            <ambient>0.5 0.5 0.5 1.0</ambient>
            <diffuse>0.5 0.5 0.5 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="wall_south">
      <static>true</static>
      <pose>0 -10 0.5 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>20 0.1 1.0</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>20 0.1 1.0</size></box></geometry>
          <material>
            <ambient>0.5 0.5 0.5 1.0</ambient>
            <diffuse>0.5 0.5 0.5 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="wall_east">
      <static>true</static>
      <pose>10 0 0.5 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>0.1 20 1.0</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.1 20 1.0</size></box></geometry>
          <material>
            <ambient>0.5 0.5 0.5 1.0</ambient>
            <diffuse>0.5 0.5 0.5 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="wall_west">
      <static>true</static>
      <pose>-10 0 0.5 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>0.1 20 1.0</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.1 20 1.0</size></box></geometry>
          <material>
            <ambient>0.5 0.5 0.5 1.0</ambient>
            <diffuse>0.5 0.5 0.5 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ==================== 영역 1: 좁은 통로 (왼쪽 위) ==================== -->
    <!-- 1.2m 폭 통로 - 회피/통과 테스트 -->
    <model name="corridor_wall_1">
      <static>true</static>
      <pose>-6 6 0.5 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>4 0.2 1.0</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>4 0.2 1.0</size></box></geometry>
          <material>
            <ambient>0.4 0.25 0.1 1.0</ambient>
            <diffuse>0.4 0.25 0.1 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="corridor_wall_2">
      <static>true</static>
      <pose>-6 4.8 0.5 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>4 0.2 1.0</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>4 0.2 1.0</size></box></geometry>
          <material>
            <ambient>0.4 0.25 0.1 1.0</ambient>
            <diffuse>0.4 0.25 0.1 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ==================== 영역 2: 장애물 회피 (오른쪽 위) ==================== -->
    <!-- 랜덤 배치된 박스 장애물 -->
    <model name="obstacle_1">
      <static>true</static>
      <pose>4 7 0.4 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>0.8 0.8 0.8</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.8 0.8 0.8</size></box></geometry>
          <material>
            <ambient>0.8 0.2 0.2 1.0</ambient>
            <diffuse>0.8 0.2 0.2 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="obstacle_2">
      <static>true</static>
      <pose>7 5 0.4 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>0.8 0.8 0.8</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.8 0.8 0.8</size></box></geometry>
          <material>
            <ambient>0.2 0.6 0.2 1.0</ambient>
            <diffuse>0.2 0.6 0.2 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="obstacle_3">
      <static>true</static>
      <pose>5.5 4 0.4 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry><box><size>0.8 0.8 0.8</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.8 0.8 0.8</size></box></geometry>
          <material>
            <ambient>0.2 0.2 0.8 1.0</ambient>
            <diffuse>0.2 0.2 0.8 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ==================== 영역 3: 열린 공간 (가운데) ==================== -->
    <!-- 로봇 스폰 위치 (0, 0) 주변 - 속도 테스트 가능 -->

    <!-- ==================== 영역 4: 움직이는 장애물 (아래쪽) ==================== -->
    <!-- 느린 이동 장애물 (0.3 m/s) - 중력/마찰 제거 -->
    <model name="moving_slow">
      <static>false</static>
      <pose>-5 -5 0.5 0 0 0</pose>
      <link name="chassis">
        <gravity>false</gravity>
        <collision name="collision">
          <geometry><box><size>0.6 0.6 0.8</size></box></geometry>
          <surface>
            <friction><ode><mu>0</mu><mu2>0</mu2></ode></friction>
          </surface>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.6 0.6 0.8</size></box></geometry>
          <material>
            <ambient>0.8 0.8 0.2 1.0</ambient>
            <diffuse>0.8 0.8 0.2 1.0</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.1</ixx><iyy>0.1</iyy><izz>0.1</izz>
          </inertia>
        </inertial>
      </link>
      <plugin filename="gz-sim-trajectory-follower-system" name="gz::sim::systems::TrajectoryFollower">
        <link_name>chassis</link_name>
        <force>100</force>
        <torque>50</torque>
        <loop>true</loop>
        <waypoint>
          <time>0</time>
          <pose>-5 -5 0.5 0 0 0</pose>
        </waypoint>
        <waypoint>
          <time>33</time>
          <pose>5 -5 0.5 0 0 0</pose>
        </waypoint>
        <waypoint>
          <time>66</time>
          <pose>-5 -5 0.5 0 0 0</pose>
        </waypoint>
      </plugin>
    </model>

    <!-- 빠른 이동 장애물 (0.8 m/s) - 중력/마찰 제거 -->
    <model name="moving_fast">
      <static>false</static>
      <pose>5 -7 0.4 0 0 0</pose>
      <link name="chassis">
        <gravity>false</gravity>
        <collision name="collision">
          <geometry><box><size>0.5 0.5 0.6</size></box></geometry>
          <surface>
            <friction><ode><mu>0</mu><mu2>0</mu2></ode></friction>
          </surface>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.5 0.5 0.6</size></box></geometry>
          <material>
            <ambient>0.8 0.4 0.1 1.0</ambient>
            <diffuse>0.8 0.4 0.1 1.0</diffuse>
          </material>
        </visual>
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.05</ixx><iyy>0.05</iyy><izz>0.05</izz>
          </inertia>
        </inertial>
      </link>
      <plugin filename="gz-sim-trajectory-follower-system" name="gz::sim::systems::TrajectoryFollower">
        <link_name>chassis</link_name>
        <force>100</force>
        <torque>50</torque>
        <loop>true</loop>
        <waypoint>
          <time>0</time>
          <pose>5 -7 0.4 0 0 0</pose>
        </waypoint>
        <waypoint>
          <time>12</time>
          <pose>-5 -7 0.4 0 0 0</pose>
        </waypoint>
        <waypoint>
          <time>24</time>
          <pose>5 -7 0.4 0 0 0</pose>
        </waypoint>
      </plugin>
    </model>

    <!-- ==================== 영역 5: 목표 마커 (각 코너) ==================== -->
    <!-- 목표 지점 시각화용 (충돌 없음) -->
    <model name="goal_marker_1">
      <static>true</static>
      <pose>-8 8 0.01 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry><cylinder><radius>0.3</radius><length>0.02</length></cylinder></geometry>
          <material>
            <ambient>0.0 1.0 0.0 1.0</ambient>
            <diffuse>0.0 1.0 0.0 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="goal_marker_2">
      <static>true</static>
      <pose>8 8 0.01 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry><cylinder><radius>0.3</radius><length>0.02</length></cylinder></geometry>
          <material>
            <ambient>0.0 1.0 0.0 1.0</ambient>
            <diffuse>0.0 1.0 0.0 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="goal_marker_3">
      <static>true</static>
      <pose>-8 -8 0.01 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry><cylinder><radius>0.3</radius><length>0.02</length></cylinder></geometry>
          <material>
            <ambient>0.0 1.0 0.0 1.0</ambient>
            <diffuse>0.0 1.0 0.0 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <model name="goal_marker_4">
      <static>true</static>
      <pose>8 -8 0.01 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry><cylinder><radius>0.3</radius><length>0.02</length></cylinder></geometry>
          <material>
            <ambient>0.0 1.0 0.0 1.0</ambient>
            <diffuse>0.0 1.0 0.0 1.0</diffuse>
          </material>
        </visual>
      </link>
    </model>

  </world>
</sdf>
